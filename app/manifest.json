{
  "manifest_version": 3,
  "name": "first-app2",
  "version": "1",
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "minimum_chrome_version": "28",
  "permissions": [ 
    "webview", 
    "storage", 
    "contextMenus", 
    "alwaysOnTopWindows", 
    "unlimitedStorage", 
    "browser"
  ]
}
