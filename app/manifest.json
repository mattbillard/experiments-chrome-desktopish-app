{
  "manifest_version": 3,
  "name": "Chrome Desktopish App",
  "version": "1",
  "icons": {
    "128": "./src/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [ 
    "webview" 
  ],
  "url_handlers": {
    "openApp": {
      "matches": [
        "http://localhost:3000/open/*"
      ],
      "title": "Open App"
    }
  }
}
